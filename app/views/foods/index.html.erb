 <div class = "container">
   <div class = "raw space"> 
   <h2>All Recipe</h2>
    <% @foods.each do |food| %>
      <%= link_to food_path(food.id) do %>
        <%= attachment_image_tag food, :image, size: "100x100", format: 'jpeg', fallback: "no_image.jpg" %> 
        <%= food.name %>
      <% end %>
    <% end %>
   </div>
 </div>